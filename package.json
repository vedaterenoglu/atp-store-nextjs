{
  "name": "atp-store-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3081",
    "build": "next build",
    "start": "next start --port 3081",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns='integration|e2e|ci'",
    "test:unit:coverage": "jest --testPathIgnorePatterns='integration|e2e|ci' --coverage",
    "test:integration": "jest --testMatch='**/integration/**/*.test.{ts,tsx}'",
    "test:integration:coverage": "jest --testMatch='**/integration/**/*.test.{ts,tsx}' --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:coverage": "playwright test --reporter=html",
    "test:ci": "npm run test:unit:coverage && npm run test:integration:coverage"
  },
  "dependencies": {
    "@clerk/localizations": "^3.20.6",
    "@clerk/nextjs": "^6.28.1",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "date-fns": "^4.1.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "react-i18next": "^15.6.1",
    "sonner": "^2.0.6",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.0.5",
    "@playwright/test": "^1.54.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-watch-typeahead": "^3.0.1",
    "lucide-react": "^0.536.0",
    "prettier": "^3.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.0",
    "typescript": "^5"
  }
}
